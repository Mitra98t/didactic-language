//boubble sort
fun boubbleSort(arr, len){
  for(var i = 0; i < len; i+=1){
    for(var j = 0; j < len - i - 1; j+=1){
      if(arr[j] > arr[j + 1]){
        var temp = arr[j];
        arr[j] = arr[j + 1];
        arr[j + 1] = temp; 
      }
    }
  }
  return arr;
}

var arr = [4,3,6,1,8,9];

print boubbleSort(arr, 6);